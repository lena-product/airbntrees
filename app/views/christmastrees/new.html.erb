<%= simple_form_for (@christmastree) do |f| %>
  <%= f.input :name %>
  <%= f.input :height %>
  <%= f.input :plant_type, :collection => %w['Balsam Fir''Fraser Fir''Canaan Fir','Douglas Fir','Grand Fir', 'Noble Fir', 'Concolor Fir', 'White Pine', 'Scotch Pine', 'Virginia Pine', 'Blue Spruce', 'Norway Spruce', 'White Spruce', 'Arizona Cypress', 'Leyland Cypress', 'Red Cedar'] %>
  <%= f.submit %>
<% end %>
